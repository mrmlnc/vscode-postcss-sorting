{
  "name": "vscode-postcss-sorting",
  "displayName": "PostCSS Sorting",
  "description": "VS Code plugin to sort CSS rules content with specified order.",
  "version": "2.0.0",
  "publisher": "mrmlnc",
  "engines": {
    "vscode": "^1.3.0"
  },
  "icon": "icon.png",
  "homepage": "https://github.com/mrmlnc/vscode-postcss-sorting",
  "categories": [
    "Other"
  ],
  "keywords": [
    "css",
    "less",
    "scss",
    "sass",
    "stylus"
  ],
  "activationEvents": [
    "onCommand:postcssSorting.sort"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "postcssSorting.sort",
      "title": "PostCSS Sorting: Run"
    }],
    "configuration": {
      "type": "object",
      "title": "Postâ€‹CSS Sorting configuration",
      "properties": {
        "postcssSorting": {
          "default": {},
          "description": "Settings for PostCSS Sorting plugin"
        }
      }
    }
  },
  "devDependencies": {
    "vscode": "^0.11.13",
    "xo": "^0.16.0"
  },
  "dependencies": {
    "postcss": "5.0.21",
    "postcss-scss": "0.1.8",
    "postcss-less": "0.14.0",
    "postcss-sorting": "1.6.1",
    "sugarss": "0.1.4"
  },
  "scripts": {
    "test": "xo"
  },
  "xo": {
    "rules": {
      "no-var": 2,
      "prefer-const": [2, { "destructuring": "all" }],
      "arrow-parens": [2, "always"],
      "babel/arrow-parens": 0,
      "object-curly-spacing": [2, "always"],
      "babel/object-curly-spacing": 0,
      "space-before-function-paren": [2, "never"],
      "generator-star-spacing": [2, "after"]
    },
    "envs": [
      "node"
    ]
  }
}

{
  "name": "vscode-postcss-sorting",
  "displayName": "PostCSS Sorting",
  "description": "VS Code plugin to sort CSS rules content with specified order.",
  "version": "2.2.1",
  "publisher": "mrmlnc",
  "license": "MIT",
  "engines": {
    "vscode": "^1.3.0"
  },
  "icon": "icon.png",
  "homepage": "https://github.com/mrmlnc/vscode-postcss-sorting/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrmlnc/vscode-postcss-sorting"
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "sorting",
    "order"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:postcssSorting.sort"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "postcssSorting.sort",
      "title": "PostCSS Sorting: Run"
    }],
    "configuration": {
      "type": "object",
      "title": "Postâ€‹CSS Sorting configuration",
      "properties": {
        "postcssSorting": {
          "default": {},
          "description": "Settings for PostCSS Sorting plugin"
        }
      }
    }
  },
  "devDependencies": {
    "vscode": "^1.0.3",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "postcss": "5.1.2",
    "postcss-scss": "0.4.1",
    "postcss-less": "0.15.0",
    "postcss-sorting": "2.0.1"
  },
  "scripts": {
    "test": "xo"
  },
  "xo": {
    "rules": {
      "no-var": 2,
      "prefer-const": [2, { "destructuring": "all" }],
      "space-before-function-paren": [2, "never"],
      "arrow-parens": [2, "always"],
      "object-curly-spacing": [2, "always"],
      "generator-star-spacing": [2, "after"],
      "import/no-unresolved": 0
    },
    "envs": [
      "node"
    ]
  }
}
